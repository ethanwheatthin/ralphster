{
  "name": "ralph-web",
  "version": "1.0.0",
  "description": "Web interface for managing multiple Ralph AI agents",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "install-all": "npm install && cd frontend && npm install"
  },
  "keywords": ["ralph", "ai", "agent", "ollama", "claude"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "cors": "^2.8.5",
    "uuid": "^9.0.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "concurrently": "^8.2.2"
  }
}
